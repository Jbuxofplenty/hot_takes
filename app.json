{
    "expo": {
        "name": "Hot Takes",
        "slug": "hot-takes",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/images/icon.png",
        "scheme": "hottakes",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "googleServicesFile": "./GoogleService-Info.plist",
            "supportsTablet": false,
            "bundleIdentifier": "com.jbuxofplenty.hottakes",
            "appleTeamId": "FWV22U8U39",
            "infoPlist": {
                "ITSAppUsesNonExemptEncryption": false,
                "CFBundleURLTypes": [
                    {
                        "CFBundleURLSchemes": ["hottakes"]
                    }
                ]
            }
        },
        "android": {
            "googleServicesFile": "./google-services.json",
            "package": "com.jbuxofplenty.hottakes",
            "runtimeVersion": {
                "policy": "appVersion"
            },
            "adaptiveIcon": {
                "backgroundColor": "#E6F4FE",
                "foregroundImage": "./src/assets/images/android-icon-foreground.png",
                "backgroundImage": "./src/assets/images/android-icon-background.png",
                "monochromeImage": "./src/assets/images/android-icon-monochrome.png"
            },
            "edgeToEdgeEnabled": true,
            "predictiveBackGestureEnabled": false
        },
        "web": {
            "output": "static",
            "favicon": "./src/assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./src/assets/images/splash-icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff",
                    "dark": {
                        "backgroundColor": "#000000"
                    }
                }
            ],
            [
                "expo-notifications",
                {
                    "icon": "./src/assets/images/splash-icon.png",
                    "color": "#2a5298"
                }
            ],
            [
                "react-native-google-mobile-ads",
                {
                    "androidAppId": "ca-app-pub-8519782324189160~6327493409",
                    "iosAppId": "ca-app-pub-8519782324189160~8486752889"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true,
            "reactCompiler": true
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "1a0f5d8b-c240-4c59-bbcd-d1fa29d2a7f1"
            }
        },
        "owner": "jbuxofplenty"
    }
}
